@model List<Instructor>

<table class="table table-hover table-bordered width =50%">
    <tr style="background-color :aquamarine">
        <td>Name</td>
        <td>Address</td>
        <td>Salary</td>
        <td>Department Name</td>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.name</td>
            <td>@item.address</td>
            <td>@item.salary</td>
            <td>@item.dept.name</td>
        </tr>
    }
</table>